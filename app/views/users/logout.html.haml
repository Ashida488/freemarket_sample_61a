- breadcrumb :logout
= render 'items/toppage/header2'
= render "layouts/breadcrumbs"
.wrapper
  = render "users/side_bar" #サイドの部分テンプレート
  .card
    .card__container
      .card-group
        =link_to destroy_user_session_path, class: "logout-btn", method: :delete do
          ログアウト
